@model List<PuestoUsuario>

    <h1>Puestos de los Usuarios</h1>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre del Puesto</th>
                <th>Descripcion</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var PuestoUsuarios in Model)
        {
            <tr>
                <td>@Html.DisplayFor(P =>PuestoUsuarios.PuestoId)</td>
                <td>@Html.DisplayFor(P =>PuestoUsuarios.PuestoNombre)</td>
                <td>@Html.DisplayFor(P =>PuestoUsuarios.PuestoDescripcion)</td>
            </tr>    
        }
    </tbody>
    </table>
